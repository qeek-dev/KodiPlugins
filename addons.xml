<?xml version="1.0" ?><addons>

    <addon id="repository.merlink.kodi-addons" name="Merlinks Unofficial Add-on Repository" provider-name="merlink" version="0.0.3">
        <extension name="Merlinks Unofficial Add-on Repository" point="xbmc.addon.repository"/>
        <extension point="xbmc.addon.metadata">
            <platform>all</platform>
            <summary>Kodi Addons from Merlink</summary>
            <description>Unofficial Addons from Merlink</description>
            <license>GNU General Public License, v2</license>
            <language/>
            <source>https://github.com/qeek-dev/KodiPlugins/tree/master/repository.merlink.kodi-addons</source>
            <datadir zip="true">https://github.com/qeek-dev/KodiPlugins/raw/master/releases</datadir>
        </extension>
    </addon>

    <addon id="script.oceanktv.youtube.dl" name="oceanktv youtube-dl" version="2020.1002.0" provider-name="qnap">
        <requires>
            <import addon="xbmc.python" version="2.1.0" />
            <import addon="script.module.addon.signals" version="0.0.1"/>
        </requires>
        <extension point="xbmc.python.script" library="control.py">
            <provides>executable</provides>
        </extension>
        <extension point="xbmc.python.module" library="lib" />
        <extension point="xbmc.addon.metadata">
            <platform>all</platform>
            <summary lang="en">Module providing access to youtube-dl video stream extraction</summary>
            <description lang="en">Oceanktv youtube-dl.</description>
            <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
            <language/>
            <source>https://github.com/qeek-dev/KodiPlugins/tree/master/script.oceanktv.youtube.dl</source>
            <datadir zip="true">https://github.com/qeek-dev/KodiPlugins/raw/master/releases</datadir>
        </extension>
    </addon>
</addons>
